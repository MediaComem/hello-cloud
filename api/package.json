{
  "name": "hello-cloud-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=hello-cloud:*,knex:query LOG_LEVEL=trace nodemon",
    "migrate": "cross-env DEBUG=knex:query knex migrate:latest",
